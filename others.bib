journal = {Proceedings of 2016 1st International Conference on Biomedical Engineering: Empowering Biomedical Technology for Better Future, IBIOMED 2016},
keywords = {Active contour without edge,Malaria parasite candidates,Segmentation,Thick blood smears},
title = {{Segmentation of malaria parasite candidates from thick blood smear microphotographs image using active contour without edge}},
year = {2017}
}

