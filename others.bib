journal = {Translational Research},
mendeley-groups = {MalariaDeepLearning},
pages = {36--55},
pmid = {29360430},
publisher = {Elsevier Inc.},
title = {{Image analysis and machine learning for detecting malaria}},
url = {https://doi.org/10.1016/j.trsl.2017.12.004},
volume = {194},
year = {2018}
}

@article{Liang2017,
abstract = {{\textcopyright} 2016 IEEE.Malaria is a major global health threat. The standard way of diagnosing malaria is by visually examining blood smears for parasite-infected red blood cells under the microscope by qualified technicians. This method is inefficient and the diagnosis depends on the experience and the knowledge of the person doing the examination. Automatic image recognition technologies based on machine learning have been applied to malaria blood smears for diagnosis before. However, the practical performance has not been sufficient so far. This study proposes a new and robust machine learning model based on a convolutional neural network (CNN) to automatically classify single cells in thin blood smears on standard microscope slides as either infected or uninfected. In a ten-fold cross-validation based on 27,578 single cell images, the average accuracy of our new 16-layer CNN model is 97.37{\%}. A transfer learning model only achieves 91.99{\%} on the same images. The CNN model shows superiority over the transfer learning model in all performance indicators such as sensitivity (96.99{\%} vs 89.00{\%}), specificity (97.75{\%} vs 94.98{\%}), precision (97.73{\%} vs 95.12{\%}), F1 score (97.36{\%} vs 90.24{\%}), and Matthews correlation coefficient (94.75{\%} vs 85.25{\%}).},
author = {Liang, Z. and Powell, A. and Ersoy, I. and Poostchi, M. and Silamut, K. and Palaniappan, K. and Guo, P. and Hossain, M.A. and Sameer, A. and Maude, R.J. and Huang, J.X. and Jaeger, S. and Thoma, G.},
doi = {10.1109/BIBM.2016.7822567},
file = {:Users/naubergois/DeepLearningMalaria/liang2016.pdf:pdf},
isbn = {9781509016105},
journal = {Proceedings - 2016 IEEE International Conference on Bioinformatics and Biomedicine, BIBM 2016},
keywords = {Computer-aided diagnosis,Convolutional neural network,Deep learning,Machine learning,Malaria},
pages = {493--496},
title = {{CNN-based image analysis for malaria diagnosis}},
year = {2017}
}

